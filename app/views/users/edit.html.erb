<div class="w-11/12 m-auto">
<%= form_for @user do |form| %>
<div class="flex flex-col">
    <%= form.label :name %>
    <%= form.text_field :name %>
</div>
<div>
      <%= form.label :about  %>
    <%= form.text_field :about %>
</div>
<div>
    <%= form.label :profileImage %>
    <%= form.file_field :profileImage %>
</div>

    <%= form.submit %>
<% end %>
</div>
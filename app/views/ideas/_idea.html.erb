<section id="<%= "#{dom_id(idea)}_container" %>">
<%= turbo_frame_tag dom_id(idea) do  %>
<section id="<%= "#{idea}_container" %>" class="w-100 flex justify-between items-center bg-indigo-400 p-2 mb-4 rounded-md">
        <h3><%= idea.name %></h3>
        <%= link_to "view" , idea_path(idea) %>
    </section>
<% end %>
</section>